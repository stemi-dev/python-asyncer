{
  "name": "@stemi-dev/python-asyncer",
  "repository": {
    "url": "git://github.com/stemi-dev/python-asyncer"
  },
  "publishConfig": {
    "@stemi-dev:registry": "https://npm.pkg.github.com"
  },
  "version": "1.0.11",
  "description": "Package used for generating tests for the python runner + converting python code to async to be run in the browser",
  "keywords": [],
  "author": "Fran Zekan",
  "license": "ISC",
  "scripts": {
    "example:test": "ts-node ./example/tests/index.ts",
    "example:all": "ts-node ./example/pyodide.ts",
    "build:ts-node": "yarn tsc -P tsconfig.node.json",
    "build:ts-browser": "yarn tsc -P tsconfig.browser.json",
    "build": "node ./scripts/build.js && yarn build:ts-node && yarn build:ts-browser",
    "build:publish": "yarn build && node ./scripts/cleanup.js",
    "clean": "rimraf node browser dist",
    "up": "yarn clean && node ./scripts/publish.js patch"
  },
  "dependencies": {
    "faker": "^5.5.3",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "@types/faker": "5.5.9"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.9",
    "browserslist-to-esbuild": "^1.0.1",
    "esbuild": "^0.13.12",
    "nodemon": "^2.0.14",
    "pyodide": "^0.18.2",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "zeko369-tsconfig": "^2.1.1",
    "zx": "^4.2.0"
  }
}
